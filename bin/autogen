#!/usr/bin/env python3

import asyncio
import pop.hub
import logging

hub_logger = logging.getLogger("pop.hub")
hub_logger.setLevel(logging.DEBUG)
hub = pop.hub.Hub()

hub.pop.sub.add(dyne_name="pkgtools", omit_class=False)

if __name__ == "__main__":
	asyncio.run(hub.pkgtools.autogen.start())

# vim: ts=4 sw=4 noet
